{
  "name": "lightning-talk-submission-platform",
  "version": "0.1.0",
  "description": "lightning-talk-submission-platform",
  "main": "server/index.js",
  "scripts": {
    "start": "webpack && node server",
    "dev": "nodemon --watch server & webpack --watch",
    "test": "jest --watchAll --noStackTrace",
    "knex": "knex --knexfile server/db/knexfile.js",
    "db:reset": "run-s db:delete db:migrate db:seed",
    "db:migrate": "npm run knex migrate:latest",
    "db:seed": "npm run knex seed:run",
    "db:delete": "rm -f server/db/dev.sqlite3",
    "postinstall": "run-s db:migrate db:seed"
  },
  "author": "Jessica Morgan",
  "license": "ISC",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "jest": {
    "setupFiles": [
      "./tests/enzyme.js"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "jest": "^24.9.0",
    "react-router-dom": "^5.0.1",
    "nodemon": "^1.18.10",
    "react": "^16.8.5",
    "react-dom": "^16.8.5",
    "react-redux": "^6.0.1",
    "redux": "^4.0.1",
    "superagent": "^5.1.0",
    "sqlite3": "^4.1.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.19.5",
    "libsodium-wrappers": "^0.7.6",
    "npm-run-all": "^4.1.5",
    "webpack": "^4.29.6"
  }
}
